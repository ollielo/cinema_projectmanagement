#!/usr/bin/env python

import h5py


with h5py.File("test.flim", "r") as f:
    for a in f.attrs:
        print("{}: {}".format(a, f.attrs[a]))

    print("channels:")
    for c in f["channels"]:
        print("  " + c)
        for a in f["channels"][c].attrs:
            print("    {}: {}".format(a, f["channels"][c].attrs[a]))

